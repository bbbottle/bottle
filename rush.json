{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.75.0",
  "pnpmVersion": "6.7.1",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "gitPolicy": {
  },
  "repository": {
    "url": "https://github.com/bbbottle/bottle.git",
     "defaultBranch": "main",
     "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "projects": [
     {
       "packageName": "@bbki.ng/components",
       "projectFolder": "packages/components"
     },
     {
       "packageName": "@bbki.ng/stylebase",
       "projectFolder": "packages/stylebase"
     },
    {
      "packageName": "@bbki.ng/site",
      "projectFolder": "packages/site"
    }
  ]
}
